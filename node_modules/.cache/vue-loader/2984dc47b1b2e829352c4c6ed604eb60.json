"shoppingcart.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA2CA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;AACA;;AAEA;AACA;AACA;AACA;AACA;;AAEA;;AAEA","file":"shoppingcart.vue","sourceRoot":"src/components","sourcesContent":["<template>\n  <div class=\"container\">\n    <div>\n    <h1 class=\"title\">All Products</h1>\n      \n      <table class=\"table is-striped\">\n        <thead>\n          <tr>\n            <th>Name</th>\n            <th>title</th>\n            <th>Price</th>\n            <th>how much</th>\n            <th>\n            </th>\n          </tr>\n        </thead>\n        <tbody>\n          <tr v-for=\"(product, index) in shoppingcart\" track-by=\"id\" v-if=\"product[4] != 0\">\n            <td>id:{{product[0]}}\n              <img :src=\"'http://194.44.152.158:8121/data/products/'+product[0]+'/'+product[3]\"  v-if=\" product[3]!=null \" alt=\"\" class=\"imgSh\"></td>\n            <td>{{product[1]}}</td>\n            <td>{{product[2]}}</td>\n            <td class=\"totalCount\">\n              <input class=\"btn   btn-info \"  @click='less(index)'  type=\"button\" value=\"-\" > {{product[4]}} \n              <input class=\"btn   btn-info \"  @click='more(index)'  type=\"button\" value=\"+\" ></td>\n            <td><input class=\"btn   btn-info btn-block \"  @click='add(index)'  type=\"button\" value=\"delete\" ></td>\n          </tr>\n           <tr>\n            <th></th>\n            <th></th>\n            <th></th>\n            <th></th>\n            <th class=\"totalCount\">Total count = {{total()}}</th>\n            <th>\n            </th>\n          </tr>\n            </th>\n        </tbody>\n    </table>\n  </div>\n  </div>\n</template>\n<script>\nexport default {\n  data () {\n    return {\n      shoppingcart: [],\n     \n    }\n  },\n  mounted () {\n   this.shoppingcart = JSON.parse(localStorage.getItem(\"myBac\")) || []  \n  },\n  methods: {\n    add: function(ind){\n     this.shoppingcart.splice(ind, 1)\n     let serialObj = JSON.stringify(this.shoppingcart);\n     localStorage.setItem(\"myBac\", serialObj)\n   },\n   less: function(ind){\n     this.shoppingcart[ind][4] -= 1\n     let serialObj = JSON.stringify(this.shoppingcart);\n     localStorage.setItem(\"myBac\", serialObj)\n     this.shoppingcart = JSON.parse(localStorage.getItem(\"myBac\"))\n   },\n   more:function(ind){\n     this.shoppingcart[ind][4] += 1\n     let serialObj = JSON.stringify(this.shoppingcart);\n     localStorage.setItem(\"myBac\", serialObj)\n     this.shoppingcart = JSON.parse(localStorage.getItem(\"myBac\")) \n   },\n   total: function(){\n     let total = 0\n    \n     for (let i=0;i < this.shoppingcart.length; i++) {\n      total += this.shoppingcart[i][2]*this.shoppingcart[i][4]\n      }\n    return total.toFixed(2)\n    }\n  \n  },\n  \n}\n</script>\n<style >\n.totalCount{\n  max-width: 40px;\n}\n.imgSh{\n  width:100px;\n}\n.imgSh:hover{\n  width: 600px;\n  position: fixed;\n  top: 15%;\n  border: 5px solid;\n  border-radius: 11px;\n\n}\n</style>"]}]}